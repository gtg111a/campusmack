<% unless signed_in? %>
  <%= link_to "Sign Up Now!", sign_up_path, :class => "button" %><br class="clear"/>
<% end %>
<% if @sotw %>
    <%= render :partial => 'sotw' %>
<% end %>
<% Conference.all.each do |conference| %>
    <%= render :partial => 'conferences/stats', :locals => { :conference => conference, :limit => 5 } %>
<% end %>
