<table class="masthead">
<tr>
	<td>
		<%= @college.name %> |
		<%= link_to "Home", college_path(@college) %> |
		<%= link_to "Smacks", college_smacks_path(@college) %> |
		<%= link_to "Redemptions", college_redemptions_path(@college) %>
	</td>
</tr>
</table>
<h1>Submit a <%= @college.name %> Post</h1>
<%= form_for [@college, @post], :html => { :multipart => true }  do |f| %> 
<%= render 'shared/fields', :f => f %>
<div class="actions">
	<%= f.submit "Submit" %> 
</div>
<% end %>

			
			
