
<%= render 'shared/masthead1' %>
	</table>	
		<h1><%= @college.name %> Smacks</h1>
			<table class="masthead">
			<tr>
				<td>
					<%= link_to "Videos", college_smacks_path(@college, :content_type => "Video")%> 
					| <%= link_to "Photos", college_smacks_path(@college, :content_type => "Photo") %> 
					| <%= link_to "News"%> 
					| <%= link_to "Stats" %>
				</td>
			</tr>
			</table>
			<% if signed_in? %>
		<a><%= button_to "Add a Smack", new_user_college_smack_path(@user, @college), :method => :get %></a>
		   <% else %>
		<a><%= button_to "Add a Smack", signin_path, :method => :get %></a>
			<% end %><br>
		<table class = "post">
		<%= render :partial => "/posts/post", :collection => @smacks %>
		</table>
		<%= will_paginate(@smacks)%>
