<h1><%= @college.name %></h1>
<%= render :partial => 'masthead' %>
<%= link_to "Add a Smack", new_college_smack_path(@college), :class => "button" %>
<%= link_to "Add a Redemption", new_college_redemption_path(@college),:class => "button" %>
<br class="clear"/>

<%= render 'shared/search_fields' %>
<% unless @posts.empty? %>
    <%= will_paginate @posts %>
    <ul class="posts">
      <%= render :partial => "/posts/post", :collection => @posts %>
    </ul>
<% end %>