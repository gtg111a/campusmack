<%= render 'shared/search_fields_redemptions' %>
<h1><%= @college.name %> Redemptions</h1>
<ul class="masthead">
  <li><%= link_to "Videos", videos_college_redemptions_path(@college) %></li>
  <li><%= link_to "Photos", photos_college_redemptions_path(@college) %></li>
  <li><%= link_to "News", news_college_redemptions_path(@college) %></li>
  <li><%= link_to "Stats", stats_college_redemptions_path(@college) %></li>
  <li class="righthead"><%= link_to "Add a Redemption", new_college_redemption_path(@college), :class => "button" %></li>
</ul>
<br class="clear"/>
<% unless @redemptions.empty? %>
    <%= will_paginate @redemptions %>
    <ul class="posts">
      <%= render :partial => "/posts/post", :collection => @redemptions %>
    </ul>
<% end %>